{
  "name": "json-cli-sorter",
  "version": "1.0.6",
  "description": "CLI tool to sort nested JSON files",
  "keywords": [
    "json",
    "cli",
    "sort"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ottigan/json-cli-sorter"
  },
  "license": "MIT",
  "author": "Ottigan",
  "bin": {
    "json-cli-sorter": "bin/index.js"
  },
  "scripts": {
    "build": "rimraf rm -r bin/* && npx tsc && rimraf rm bin/index.js.map",
    "debug": "npx tsc && node --inspect=6001 bin/index.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
